                    <strong>
                      <%- article.category %>
                    </strong>
                  </div>
                </div>
              </div>
              <div>
                <div class="w3-tag w3-round w3-orange mx-1" style="padding:5px">
                  <div class="w3-tag w3-round w3-orange w3-border w3-border-white">
                    <strong>閱讀時間 <%- article.readingTime%> Min</strong>
                  </div>
                </div>
              </div>
              <% for(var i=0;i<article.tags.length;i++){ %>
                <div class="w3-tag w3-round w3-blue mx-1" style="padding:5px">

                  <strong>
                    <%= article.tags[i] %>
                  </strong>

                </div>
                <% } %>


            </div>
          </div>
        </div>

